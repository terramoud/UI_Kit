include ../Common.blocks/Content/__ColorDescriptionWrapper/Content__ColorDescriptionWrapper.pug
include ../Common.blocks/Content/__ColorNameDescription/Content__ColorNameDescription.pug
include ../Common.blocks/Content/__ColorName/Content__ColorName.pug
include ../Common.blocks/Content/__ColorSwatchWrapper/Content__ColorSwatchWrapper.pug
include ../Common.blocks/Content/__WrapperRight/Content__WrapperRight.pug
include ../Common.blocks/Logo/Logo.pug
include ../Common.blocks/Content/Content.pug
